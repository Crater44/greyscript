{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "GreyScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#number"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.greyscript",
					"match": "\\b(if|while|for|function|then|return|end if|end for|end while|end function|else|and|or|in|not|continue|break|new|null)\\b"
				},
				{
					"name": "support.class",
					"match": "\\b(Shell|FtpShell|Computer|File|Router|NetSession|Metaxploit|Metalib|Port|Crypto|int|float|string|String|Int|Float|bool|map|Map|list|List)\\b"
				},
				{
					"name": "support.variable.shell",
					"match": "\\.(host_computer)"
				},
				{
					"name": "support.function.shell",
					"match": "\\.(scp|start_terminal|build|connect_service|launch|ping)"
				},
				{
					"name": "support.function.aptclient",
					"match": "\\.(show|search|update|add_repo|del_repo|install|check_upgrade)"
				},
				{
					"name": "support.function.metamail",
					"match": "\\.(delete|fetch|read|send)"
				},
				{
					"name": "support.variable.ftpshell",
					"match": "\\.(host_computer)"
				},
				{
					"name": "support.function.ftpshell",
					"match": "\\.(start_terminal|put)"
				},
				{
					"name": "support.variable.computer",
					"match": "\\.(public_ip|local_ip|active_net_card|get_ports|groups|is_network_active|network_devices|network_gateway)"
				},
				{
					"name": "support.function.computer",
					"match": "\\.(File|touch|create_user|delete_user|create_group|delete_group|change_password|close_program|connect_ethernet|connect_wifi|wifi_networks|create_folder|show_procs)"
				},
				{
					"name": "support.variable.file",
					"match": "\\.(is_binary|is_folder|size|name|owner|parent|group|permissions|path)"
				},
				{
					"name": "support.function.file",
					"match": "\\.(delete|set_content|get_content|set_group|set_owner|get_files|get_folders|has_permission|chmod|rename|move|copy)"
				},
				{
					"name": "support.function.metaxploit",
					"match": "\\.(load|net_use|scan_address|scan|sniffer|rshell_server|rshell_client)"
				},
				{
					"name": "support.variable.metalib",
					"match": "\\.(lib_name|version)"
				},
				{
					"name": "support.function.metalib",
					"match": "\\.(overflow)"
				},
				{
					"name": "support.function.crypto",
					"match": "\\.(aircrack|airmon|decipher|smtp_user_list|aireplay)"
				},
				{
					"name": "support.variable.port",
					"match": "\\.(get_lan_ip|is_closed|port_number)"
				},
				{
					"name": "support.function.netsession",
					"match": "\\.(dump_lib)"
				},
				{
					"name": "support.variable.router",
					"match": "\\.(public_ip|local_ip|device_ports|devices_lan_ip|port_info|used_ports|bssid_name|essid_name|firewall_rules|kernel_version)"
				},
				{
					"name": "support.function.router",
					"match": "\\.(ping_port)"
				},
				{
					"name": "support.variable.general",
					"match": "\\b(time|current_date|parent_path|home_dir|program_path|active_user|user_mail_address|user_bank_number|current_path|launch_path)"
				},
				{
					"name": "support.function.general",
					"match": "\\b(clear_screen|print|typeof|md5|get_switch|get_router|get_shell|nslookup|whois|is_valid_ip|is_lan_ip|command_info|format_columns|user_input|include_lib|import_code|exit|bitwise|wait|hash|mail_login)"
				},
				{
					"name": "support.function",
					"match": "((\\b(slice|abs|acos|asin|atan|tan|cos|sin|char|floor|round|range|rnd|sign|sqrt|str|ceil)\\(|\\.(split|indexOf|hasIndex|lower\b|upper\b|remove|lastIndexOf|replace|trim|indexes\b|code\b|len\b|val\b|values\b|to_int\b|join|push|pop|pull|shuffle|reverse|sort|sum\b))|\bpi\b)"
				},
				{
					"name": "support.type",
					"match": "\\b(params|true|false|globals|self)"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.greyscript",
			"begin": "\"",
			"end": "\""
		},
		"number": {
			"patterns": [{
				"name": "constant.numeric",
				"match": "\\d"
			}]
		},
		"comment": {
			"name": "comment.line",
			"begin": "//",
			"end": "\n"
		}
	},
	"scopeName": "source.src"
}
